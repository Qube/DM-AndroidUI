<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="agh.to2.dicemaster.view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="agh.to2.dicemaster.view.LoginView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="agh.to2.dicemaster.view.LobbyView" />
        <activity android:name="agh.to2.dicemaster.view.TableCreatorView"  android:noHistory="true"/>
        <activity android:name="agh.to2.dicemaster.view.PlayerTableView" />
        <activity android:name="agh.to2.dicemaster.view.ObserverTableView"></activity>
    </application>

</manifest>